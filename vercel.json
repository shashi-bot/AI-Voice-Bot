{
    "version": 2,
    "builds": [
      {
        "src": "public/**",
        "use": "@vercel/static"
      },
      {
        "src": "api/voicebot.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/voicebot.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }